    "version": "0.2.0",
    "configurations": [
        {"name":"Python: Django","type":"python","request":"launch","program":"${workspaceFolder}\\manage.py","args":["runserver"],"django":true},
    
        {
        "type": "chrome",
        "request": "launch",
        "name": "vuejs: chrome",
        "url": "http://localhost:8081",
        "webRoot": "${workspaceFolder}/frontend/testui",
        "breakOnLoad": true,
        "pathMapping": {
            "/_karma_webpack_": "${workspaceFolder}/frontend/testui"
        },
        "sourceMapPathOverrides": {
            "webpack:/*": "${webRoot}/*",
            "/./*": "${webRoot}/*",
            "/src/*": "${webRoot}/*",
            "/*": "*",
            "/./~/*": "${webRoot}/node_modules/*"
        },
        "preLaunchTask": "vuejs: start"
        }
    ]
}